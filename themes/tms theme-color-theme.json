{
	"name": "tms theme",
	"type": "light",
	"colors": {
		"editor.background": "#dce8f2",
		//sidebar
		"sideBarSectionHeader.background": "#013b56",
		"sideBarTitle.foreground": "#ffffff",
		"sideBarSectionHeader.foreground": "#ffffff",
		"sideBar.border": "#3376b7",
		"sideBar.background": "#013b56",
		"sideBar.foreground": "#ffffff",
		 //tab
		"tab.inactiveBackground": "#dce8f2",
		"tab.inactiveForeground": "#5e5d5d",
		"tab.activeBackground": "#013b56",
		"tab.activeForeground": "#ffffff",
		"statusBar.background":"#013b56",
		"activityBar.background":"#013b56",
		//output panel stuff
		"panel.background":"#dce8f2",
		"panel.border":"#3376b7",
		"panel.dropBorder": "#5e5d5d",
		"panelInput.border": "#013b56",
		"panelTitle.activeBorder":"#013b56",
		//activity bar
		"activityBar.foreground": "#dce8f2",
		"activityBar.inactiveForeground":"#dce8f2",
		"activityBarBadge.background":"#5db65f",
		"activityBar.activeFocusBorder":"#013b56",
		"titleBar.activeBackground": "#013b56",
		"titleBar.activeForeground": "#ffffff",
		"titleBar.inactiveBackground": "#013b56",
		"titleBar.inactiveForeground": "#013b56",
		"titleBar.border": "#013b56",
		"menu.background": "#013b56",
